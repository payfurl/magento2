<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp/etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="payfurl-js-sdk" type="host">*.payfurl.com</value>
            </values>
        </policy>
        <policy id="connect-src">
            <values>
                <value id="payfurl-api-connect" type="host">*.payfurl.com</value>
            </values>
        </policy>
        <policy id="style-src">
            <values>
                <value id="payfurl-form-style" type="host">*.payfurl.com</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="payfurl-frame" type="host">*.payfurl.com</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
